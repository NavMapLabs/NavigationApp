{
  "name": "navigationapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --coverage=true",
    "lint": "expo lint",
    "deploy": "gh-pages -t -d dist",
    "deploy-github-action": "gh-pages -d dist -t true -r https://x-access-token:${{ secrets.GITHUB_TOKEN }}@github.com/${{ github.repository }}.git",
    "predeploy": "expo export -p web",
    "build_dev_image": "docker build -t nav_app_dev:1.0 .",
    "run_dev_image": "docker run -p 3000:3000 nav_app_dev:1.0",
    "separator": "------- don't mind this, will be remove ---------",
    "build_image_CI_CD": "docker build -t nav_app_test:1.0 .",
    "prebuild_image_CI_CD": "npx expo export -p web",
    "run_image_CI_CD": "docker run -p 3000:3000 nav_app_test:1.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "scripts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.|@expo-google-fonts/.|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|react-redux)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.6",
    "@types/express": "^4.17.21",
    "@types/immutable": "^3.8.7",
    "@types/react-native": "^0.73.0",
    "dotenv": "^16.4.5",
    "expo": "~51.0.18",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.17",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "firebase": "^10.12.4",
    "immutable": "^4.3.7",
    "module": "^1.1.0",
    "navigationapp": "file:",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-canvas": "^0.1.40",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.4.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "^13.10.5",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^9.1.2",
    "react-responsive": "^10.0.0",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.5",
    "@types/react": "~18.2.45",
    "@types/react-native-canvas": "^0.1.13",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "compression": "^1.7.4",
    "express": "^4.21.0",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "morgan": "^1.10.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4"
  },
  "private": true
}
